{
  "name": "coding-challenge-backend-c",
  "version": "0.0.0",
  "description": "coding challenge in Nodejs",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config ./build/app/index.js",
    "build": "rm -rf build && ./node_modules/typescript/bin/tsc",
    "dev": "NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js --watch app -e ts --exec 'ts-node -r dotenv/config app/index.ts'",
    "refreshData": "node ./build/updateFirestoreFromTsv.js",
    "lint": "./node_modules/.bin/eslint . --ext .ts --ignore-pattern node_modules/",
    "lint:fix": "./node_modules/.bin/eslint --fix . --ext .ts --ignore-pattern node_modules/",
    "test": "npm run test:unit && npm run test:functional",
    "test:unit": "NODE_ENV=test mocha -r ts-node/register -r reflect-metadata 'tests/unit/**/*.ts'  --bail",
    "test:functional": "NODE_ENV=test mocha -r ts-node/register -r reflect-metadata 'tests/functional/**/*.ts' --bail",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --report-dir=tests/reports/coverage mocha --exit -r ts-node/register 'tests/{,!(mock)/**}/*.ts' -r reflect-metadata && nyc check-coverage --statements 95 --branches 95 --functions 95",
    "coverage:unit": "NODE_ENV=test nyc --report-dir tests/coverage/unit --reporter lcov npm run test:unit",
    "coverage:functional": "NODE_ENV=test nyc --report-dir tests/coverage/functional --reporter lcov npm run test:functional"
  },
  "author": "",
  "license": "WTFPL",
  "dependencies": {
    "@google-cloud/firestore": "^6.4.3",
    "@type-cacheable/core": "^11.0.1",
    "@type-cacheable/redis-adapter": "^14.0.2",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "chai": "^1.10.0",
    "csv-parse": "^5.3.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "^0.0.1-security",
    "haversine-distance": "^1.2.1",
    "inversify": "^6.0.1",
    "inversify-restify-utils": "^3.6.0",
    "jaro-winkler": "^0.2.8",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "pino": "^8.11.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "restify": "^11.1.0",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "ts-sinon": "^2.0.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "uuidv4": "^6.2.13"
  },
  "engines": {
    "node": "0.10.26"
  },
  "nyc": {
    "exclude": [
      "app/Kernel.ts",
      "app/App.ts",
      "tests/**/*.ts"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/jaro-winkler": "^0.2.1",
    "@types/mocha": "^10.0.1",
    "@types/pino": "^7.0.5",
    "@types/restify": "^8.5.6",
    "@types/restify-errors": "^4.3.5",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5",
    "dotenv": "^16.0.3"
  }
}
